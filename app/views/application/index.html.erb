<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Ember Order</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <base href="/">

    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" href="ember-assets/vendor.css">
    <link rel="stylesheet" href="ember-assets/client.css">
  <%= csrf_meta_tags %></head>
  <body><%= yield %>
    <script src="http://localhost:3001/socket.io/socket.io.js"></script>
    <script>
      window.socket = io.connect('http://localhost:3001');
    </script>
    <script src="ember-assets/vendor.js"></script>
    <script src="ember-assets/client.js"></script>
    <script>
      window.ClientENV = require('client/config/environment')['default'];
      window.EmberENV = window.ClientENV.EmberENV;
      window.Client = require('client/app')['default'].create(ClientENV.APP);
    </script>
    <script>
      $( document ).ready( function() {
        $( document ).foundation();
      });
    </script>
    <script src="//use.typekit.net/xwe6rug.js"></script>
    <script>try{Typekit.load();}catch(e){}</script>
  </body>
</html>
